{
    "production": {
        "app_function": "lambda_handler.handler",
        "aws_region": "eu-west-1",
        "project_name": "mock-mcp-server",
        "runtime": "python3.11",
        "s3_bucket": "zappa-mock-mcp-port",
        "memory_size": 4096,
        "timeout_seconds": 300,
        "keep_warm": true,
        "keep_warm_expression": "rate(5 minutes)",
        "cors": true,
        "binary_support": false,
        "exclude": [
            ".git/*",
            ".github/*",
            "__pycache__/*",
            "*.pyc",
            ".venv/*",
            "venv/*",
            "tests/*",
            "*.md",
            "Dockerfile",
            "docker-compose.yml",
            "render.yaml",
            "apprunner.yaml"
        ]
    }
}
